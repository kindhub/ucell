extends ./pug/layout

block content
	<!-- START HEADER -->
	.header-mobile 
		.container
			.header-mobile__inner 
				a.header-mobile__logo-wrapper
					img.header-mobile__logo(src="images/logo.svg")
				.header__nav.nav
					a.nav-item.content-tab.s-nav__btn(href="#home") Главная
					a.nav-item.content-tab.s-nav__btn(href="#lead") Знакомься с руководством
					a.nav-item.content-tab.s-nav__btn(href="#growth") Твое развитие
					a.nav-item.content-tab.s-nav__btn(href="#we") Мы первые
					a.nav-item.content-tab.s-nav__btn(href="#history") История успеха
